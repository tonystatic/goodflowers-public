        "hammerjs": "^2.0.8",
        "inputmask": "^5.0.0",
        "jquery": "^3.4.1",
        "js-cookie": "^2.2.1",
        "laravel-mix": "^5.0.0",
        "moment": "^2.24.0",
        "postcss-discard-comments": "^4.0.2",
        "precss": "^4.0.0",
        "sass": "^1.23.3",
        "sass-loader": "^8.0.0",
        "svg-pan-zoom": "ariutta/svg-pan-zoom"
    }
